
@{
    ViewBag.Title = "IndexHome";
}

<div class="container-fluid" ng-app="phonecatApp">
    <div ng-controller="PhoneListController">

        <ul>
            <li ng-repeat="phone in phones">
                <span>
                    {{phone.name}}
                </span>
                <p>
                    {{phone.snippet}}
                </p>
            </li>
        </ul>

    </div>
</div>